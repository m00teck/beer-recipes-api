<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseUnauthorized">
    <DisplayName>RaiseUnauthorized</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json"/>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
            <Payload type="application/json">
                {
                    "message": "Please provide a valid API key for the given resource."
                }
            </Payload>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>